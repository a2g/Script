{
  "name": "yeoman-generated",
  "version": "0.0.0",
  "description": "yeoman-generated",
  "license": "MIT",
  "repository": "",
  "author": {
    "name": "",
    "email": "blah@blah.com",
    "url": "http://url.com"
  },
  "keywords": [
    "redis",
    "nodejs",
    "example",
    "github"
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index",
  "types": "lib/index",
  "scripts": {
    "app": "node lib/src/app.js",
    "build": "tsc",
    "clean": "rimraf lib && rimraf coverage",
    "compile": "tsc",
    "coverage": "jest --coverage",
    "format-old": "prettier --write \"{src,testsYup}/**/*.ts\" --single-quote --trailing-comma es5",
    "lint-old": "eslint -c .eslintrc.js --ext .ts src/**/*.ts",
    "lint": "eslint --ext .ts,.html . --ignore-path .gitignore && prettier \"**/*.ts\" --check --ignore-path .gitignore",
    "format": "eslint --ext .ts,.html . --fix --ignore-path .gitignore && prettier \"**/*.ts\" --write --ignore-path .gitignore",
    "cli": "tsc --pretty && node lib/src/cli.js",
    "prepublishOnly": "npm run build",
    "prebuild": "npm run clean && npm run format && npm run lint && echo Using TypeScript && tsc --version",
    "pretty": "tsc --pretty",
    "serve": "node lib/src/serve.js",
    "serveRepos": "node lib/src/serveRepos.js",
    "test": "jest",
    "watch": "tsc -w & nodemon lib/src/serveSolutions.js",
    "watch:test": "jest --watch"
  },
  "devDependencies": {
    "@svgdotjs/svg.js": "^3.1.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^23.3.14",
    "@types/node": "^10.11.4",
    "@types/prompt-sync": "^4.2.0",
    "@types/redis": "^2.8.28",
    "@types/response-time": "^2.3.5",
    "@open-wc/eslint-config": "^4.3.0",
    "@open-wc/testing": "next",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "coveralls": "^3.0.2",
    "jest": "^29.5.0",
    "prompt-sync": "^4.2.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^7.0.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.32.0",
    "prettier": "^2.4.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2",
    "ts-standard": "^11.0.0"
  },
  "dependencies": {
    "axios": "^1.3.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "redis": "3.0.2",
    "response-time": "^2.3.2",
    "svgdom": "^0.1.14",
    "xmlbuilder2": "^3.1.0",
    "xmldom": "^0.6.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "customElements": "custom-elements.json",
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "@open-wc",
      "prettier"
    ],
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": [
        "error"
      ],
      "import/no-unresolved": "off",
      "import/extensions": [
        "error",
        "always",
        {
          "ignorePackages": true
        }
      ]
    }
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
