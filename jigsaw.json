            "required": [
              "goal1",
              "inv1"
            ]
          }
        },
        {
          "if": {
            "properties": {
              "piece": {
                "examples": [
                  "search for it"
                ],
                "const": "AUTO_PROP1_BECOMES_PROP2_BY_PROPS"
              }
            }
          },
          "then": {
            "required": [
              "prop1",
              "prop2"
            ]
          }
